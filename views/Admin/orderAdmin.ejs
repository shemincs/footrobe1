    <script>


const orderBtns = document.querySelectorAll('.change-order-btn');

orderBtns.forEach((btn) => {
    btn.addEventListener('click', async () => {
        const selectElement = btn.parentElement.previousElementSibling.querySelector('.status-select');
        const selectedValue = selectElement.value;
        const orderId = btn.dataset.id;
        console.log(selectedValue)
        const postData = {
            order_id: orderId,
            status: selectedValue // Pass the selected value here
        };

        const requestOptions = {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify(postData)
        };

        try {
            const res = await fetch('/admin/change-status', requestOptions);
            const data = await res.json();
            success:(response)=>{
            if(response.status===true){
             location.reload
            }
                    }
            // Handle the response data as needed
        } catch (error) {
            // Handle any errors
        }
    });
});


